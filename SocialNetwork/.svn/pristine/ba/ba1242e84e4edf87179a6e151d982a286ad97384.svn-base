package Dominion;

import java.sql.SQLException;


public class PublisherReviewer{
	
	protected String publisher_mail;
	protected String reviewer_mail;
	
	
	
	public PublisherReviewer(String publisher, String reviewer) throws SQLException{
		this.publisher_mail=publisher;
		this.reviewer_mail=reviewer;
	}
	
	public PublisherReviewer(){
	}
	
	
	
	public String getPublisher_mail() {
		return publisher_mail;
	}

	public void setPublisher_mail(String publisher_mail) {
		this.publisher_mail = publisher_mail;
	}

	public String getReviewer_mail() {
		return reviewer_mail;
	}

	public void setReviewer_mail(String reviewer_mail) {
		this.reviewer_mail = reviewer_mail;
	}

	@Override
	public String toString() {
		return "PublisherReviewer [Publisher=" + publisher_mail + ", Reviewer=" + reviewer_mail + "]\n";
	}

}
